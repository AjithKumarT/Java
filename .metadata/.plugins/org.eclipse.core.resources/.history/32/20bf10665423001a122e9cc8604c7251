package LanguageBasics;

import java.util.Arrays;

public class RemoveDuplicate {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a[]={10,20,30,10,40,20};
		Arrays.sort(a);
		int j=0;
		  for (int i=0; i < a.length; i++){  
			  int k=i+1;
	           if(k<a.length){ 
	            	if(a[i]!=a[k]){
	            		 a[j++] = a[i];  
	            	}
	               
	            }  k++;
	        }
		  for(int i:a){
			  System.out.print(i+" ");
		  }

	}

}
