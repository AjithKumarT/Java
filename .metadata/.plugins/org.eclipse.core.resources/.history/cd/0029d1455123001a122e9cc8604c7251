package LanguageBasics;

public class RemoveDuplicate {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a[]={10,20,30,10,40,20};
		int j=0;
		  for (int i=0; i < a.length; i++){  
	            if (a[i] != a[i+1]){  
	                a[j++] = a[i];  
	            }  
	        }
		  for(int i:a){
			  System.out.print(i+" ");
		  }

	}

}
