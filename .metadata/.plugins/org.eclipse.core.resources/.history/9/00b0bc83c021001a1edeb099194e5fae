package LanguageBasics;

public class ArrayPrograms {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a[]= {1,3,5,7,9};
		int b[]=new int[5];
		int d[]=a;
		System.arraycopy(a, 1, b, 0, a.length);
		for(int c:b){
			System.out.println(c);
		}
		for(int e:d){
			System.out.println(e);
		}
			

	}

}
